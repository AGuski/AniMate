{
  "manifest_version": 2,

  "name": "AniMAte",
  "description": "This app is pretty awesome.",
  "version": "0.0.1",
  "minimum_chrome_version": "10.0",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "storage",
    {"fileSystem": ["write", "retainEntries", "directory"]} 
  ],
  "icons": { 
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }
}